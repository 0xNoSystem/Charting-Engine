{
  "name": "kwant",
  "version": "0.2",
  "description": "React Binance candlestick chart component.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./src/index.css",
    "./dist/index.css"
  ],
  "scripts": {
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/index.css",
    "build": "npm run build:css && tsup && npm run build:css",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.0.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18"
  }
}
